{
    "name": "<%= config.libraryName.slugified %>",
    "version": "0.0.0",
    "authors"        : [
        {
            "name"    : "<%= config.author.name %>",
            "email"   : "<%= config.author.email %>"
        }
    ],
    "main": ["dist/<%= config.libraryName.slugified %>.js"],
    "ignore": [
        "src",
        "test",
        "Gruntfile.js",
        "**/.*"
    ],
    "dependencies": {
        "angular": "~1.2.20"<% if (config.includeAngularModuleResource) { %>,
        "angular-resource": "~1.2.20"<% } %><% if (config.includeAngularModuleCookies) { %>,
        "angular-cookies": "~1.2.20"<% } %><% if (config.includeAngularModuleSanitize) { %>,
        "angular-sanitize": "~1.2.20"<% } %>
    },
    "devDependencies": {
        "angular-mocks": "~1.2.20",
        "angular-scenario": "~1.2.20"
    }
}
